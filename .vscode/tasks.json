{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Flutter-Launch-All",
            "dependsOn": [
                "Flutter-Launch-Windows",
                "Flutter-Launch-Android"
            ]
        },
        {
            "label": "Flutter-Launch-Windows",
            "type": "shell",
            "command": "flutter run -d 'windows'"
        },
        {
            "label": "Flutter-Launch-Android",
            "type": "shell",
            "command": "flutter run -d 'ce0716075b9e140e05'"
        },
        {
            "label": "Flutter-Launch-Chrome",
            "type": "shell",
            "command": "flutter run -d 'chrome'"
        },
        {
            "label": "watch build_runner automatically run on startup",
            "type": "shell",
            "command": "dart run build_runner watch",
            "presentation": {
                "reveal": "never",
                "panel": "dedicated"
            },
            "runOptions": {
                "runOn": "folderOpen"
            }
        }
    ]
}